{
    "name": "foostart/package-filemanager",
    "description": "A file upload/editor intended for use with Laravel 5 and CKEditor / TinyMCE",
    "license": "MIT",
    "keywords": [
        "filemanager",
        "laravel",
        "ckeditor",
        "tinymce",
        "upload",
        "file",
        "manager",
        "image"
    ],
    "authors": [
        {
            "name": "foostart",
            "email": "foostart.com@gmail.com",
            "homepage": "http://www.foostart.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "illuminate/support": ">=4.2.0",
        "intervention/image": "2.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~4.8"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "autoload": {
        "psr-4": {
            "Foostart\\Filemanager\\": "src/"
        }
    },
    "bin": [
        "bin/debug"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Foostart\\Filemanager\\FilemanagerServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
